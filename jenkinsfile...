pipeline{
    agent any
    stages{
        stge ('deploy'){
            environment{
                awsfile=credentials('awsfile')
            }
            steps{
                sh '''
                ssh -i $awsfile -o stricthostkeychecking=no ubuntu@3.21.41.81 'sudo bash -s' < myscript.sh'''
                
            }
        }
    }
}

            
        
            
